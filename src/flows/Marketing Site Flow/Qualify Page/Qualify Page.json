{
  "name": "c517582b-e348-4ffa-97ca-f9e50db61d4d",
  "displayName": "Qualify Page",
  "description": "Light qualification: campaign_status and visitor_role",
  "entryFulfillment": {
    "messages": [
      {
        "text": {
          "text": [
            "To help you best, I'd like to understand your situation."
          ]
        }
      }
    ]
  },
  "form": {
    "parameters": [
      {
        "displayName": "campaign_status",
        "entityType": "campaign_status",
        "fillBehavior": {
          "initialPromptFulfillment": {
            "messages": [
              {
                "text": {
                  "text": [
                    "Are you currently running an **active campaign**, in the **planning** stage, or just **researching** options?"
                  ]
                }
              }
            ]
          },
          "repromptEventHandlers": [
            {
              "event": "sys.no-match-default",
              "triggerFulfillment": {
                "messages": [
                  {
                    "text": {
                      "text": [
                        "I didn't catch that. Are you active, planning, or researching?"
                      ]
                    }
                  }
                ]
              }
            }
          ]
        },
        "required": true,
        "redact": false,
        "advanced Settings": {}
      },
      {
        "displayName": "visitor_role",
        "entityType": "visitor_role",
        "fillBehavior": {
          "initialPromptFulfillment": {
            "messages": [
              {
                "text": {
                  "text": [
                    "Are you the **decision maker**, a **recommender**, or **researching** for your team?"
                  ]
                }
              }
            ]
          },
          "repromptEventHandlers": []
        },
        "required": false,
        "redact": false,
        "advancedSettings": {}
      }
    ]
  },
  "transitionRoutes": [
    {
      "condition": "$session.params.campaign_status = \"active\" AND $session.params.visitor_role = \"decision_maker\"",
      "triggerFulfillment": {
        "setParameterActions": [
          {
            "parameter": "lead_tier",
            "value": "hot"
          }
        ],
        "messages": [
          {
            "text": {
              "text": [
                "Perfect! Since you're actively running a campaign, let's get you scheduled for a demo."
              ]
            }
          }
        ]
      },
      "targetPage": "demo-capture-page"
    },
    {
      "condition": "$session.params.campaign_status = \"active\" OR $session.params.campaign_status = \"planning\"",
      "triggerFulfillment": {
        "setParameterActions": [
          {
            "parameter": "lead_tier",
            "value": "warm"
          }
        ],
        "messages": [
          {
            "text": {
              "text": [
                "Great! Would you like to book a quick demo or should I email you more information?"
              ]
            }
          }
        ]
      },
      "targetPage": "entry-page"
    },
    {
      "condition": "true",
      "triggerFulfillment": {
        "setParameterActions": [
          {
            "parameter": "lead_tier",
            "value": "cold"
          }
        ],
        "messages": [
          {
            "text": {
              "text": [
                "Thanks! I can email you a product overviewâ€”what's your email address?"
              ]
            }
          }
        ]
      },
      "targetPage": "email-capture-page"
    }
  ],
  "eventHandlers": []
}
